local Tab = Window.NewTab("Toggles")
local Section = Tab.NewSection("Color Fade Shit")

--Wolf
--Made by: !Kryo the lazy mothafucker
_G.Wolf4Fade = false
local frequency = 0.5
local amplitude = 0.5
local offset1 = {_G.D, _G.D2, _G.D3} -- Starting color: black
local offset2 = {_G.E, _G.E2, _G.E3} -- First color
local offset3 = {_G.G, _G.G2, _G.G3} -- Second color
local offset4 = {_G.H, _G.H2, _G.H3} -- Third color
local offset5 = {_G.I, _G.I2, _G.I3}
local offset6 = {_G.J, _G.J2, _G.J3}
local transitionTime = _G.D1 or 5 -- Time (in seconds) for each transition between colors

local RS = game:GetService('ReplicatedStorage')
local mk1 = RS:FindFirstChild('MasterKey')

local colors = {offset1, offset2, offset3, offset4, offset5, offset6}
local colors2 = {offset6, offset1, offset2, offset3, offset4, offset5}
local numColors = #colors
local numColors2 = #colors2

local EnabledToggle = Section.NewToggle("Custom RGB4 Name/Wolf/Particles!", function(bool)
    if _G.Wolf4Fade then
        _G.Wolf4Fade = false
        return
    else
        _G.Wolf4Fade = true
    end

	while _G.Wolf4Fade do

    	local startTime = tick()
    	local endTime = startTime + transitionTime

    	while tick() < endTime do
    	    local currentTime = tick() - startTime
    	    local transitionProgress = currentTime / transitionTime
    	    local colorIndex = math.floor(transitionProgress * numColors) + 1
    	    local transitionRatio = (currentTime % (transitionTime / numColors)) / (transitionTime / numColors)

	    	local color1 = colors[colorIndex]
	    	local color2 = colors[(colorIndex % numColors) + 1]

	   		local red = math.floor(color1[1] * (1 - transitionRatio) + color2[1] * transitionRatio)
	    	local green = math.floor(color1[2] * (1 - transitionRatio) + color2[2] * transitionRatio)
        	local blue = math.floor(color1[3] * (1 - transitionRatio) + color2[3] * transitionRatio)

    		local color = Color3.fromRGB(red, green, blue)

			
			local args = {
    			[1] = "ChangeColor",
    			[2] = {
       			    color.R,
       			    color.G,
       			    color.B,
       			}
   			}
   			mk1:FireServer(unpack(args))

			local G_5 = "ColorParticle"
   			game:GetService("ReplicatedStorage").MasterKey:FireServer(G_5, color)

        	task.wait()
    	end
	end	
end)

local function applyColorTransition()
    local offset1 = {_G.D, _G.D2, _G.D3} -- Starting color: black
    colors[1] = offset1 -- Update the colors with the new offset1
end    
local function applyColorTransition1()
    local offset2 = {_G.E, _G.E2, _G.E3} -- Starting color: black
    colors[2] = offset2 -- Update the colors with the new offset1
end
local function applyColorTransition2()
    local offset3 = {_G.G, _G.G2, _G.G3} -- Starting color: black
    colors[3] = offset3 -- Update the colors with the new offset1
end
local function applyColorTransition3()
    local offset4 = {_G.H, _G.H2, _G.H3} -- Starting color: black
    colors[4] = offset4 -- Update the colors with the new offset1
end
local function applyColorTransition4()
    local offset5 = {_G.I, _G.I2, _G.I3} -- Starting color: black
    colors[5] = offset5 -- Update the colors with the new offset1
end
local function applyColorTransition5()
    local offset6 = {_G.J, _G.J2, _G.J3} -- Starting color: black
    colors[6] = offset6 -- Update the colors with the new offset1
end
local function resetSpeed()
    local transitionTime = _G.D1 or 5-- Starting color: black
end

local text1_ = Section.Newtextbox('Color1 (rgb)! 0 - 255', function(self, value)
    _G.D = value
    applyColorTransition() -- Reload the colors (update offset1)
end)

