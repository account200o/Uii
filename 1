local EnabledToggle = Section.NewToggle("SpamLights!", function(bool)
    if _G.SpamLights then
        _G.SpamLights = false
        return
    else
        _G.SpamLights = true
    end

    while _G.SpamLights do
        for _, L in next, workspace.Models:GetDescendants() do
            if L:IsA("ClickDetector") then
                fireclickdetector(L)
            end
        end
        task.wait(0.5)
    end
end)

